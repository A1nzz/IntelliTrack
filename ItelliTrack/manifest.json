{
    "manifest_version": 3,
    "name": "IntelliTrack",
    "version": "1.0",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/favicon-16x16.png",
        "32": "img/favicon-32x32.png"
      }
    }
  }